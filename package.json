{
  "name": "isomorphic-redux-app",
  "version": "0.1.1",
  "description": "An example of a isomorphic Redux application",
  "scripts": {
    "slate": "rm -rf node_modules && npm install",
    "clean": "rm -rf dist",
    "start": "NODE_ENV=development node src/server/index.js --progress --colors --profile",
    "debugger": "node debug src/server/index.js --progress --colors --profile",
    "start-prod": "NODE_ENV=production node src/server/index.js --progress --colors --profile",
    "build": "npm run clean && NODE_ENV=production webpack -p --progress --colors --profile",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caljrimmer/isomorphic-redux-app.git"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.0",
    "axios": "^0.7.0",
    "babel": "^5.8.21",
    "body-parser": "^1.14.1",
    "classnames": "^2.1.5",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.0",
    "es6-promise": "^3.0.2",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "history": "^1.9.0",
    "isomorphic-fetch": "^2.2.0",
    "moment": "^2.10.6",
    "mongoose": "^4.2.8",
    "morgan": "^1.6.1",
    "node-fetch": "^1.3.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.3",
    "react": "^0.14.5",
    "react-bootstrap": "^0.28.5",
    "react-dom": "^0.14.7",
    "react-redux": "^4.1.2",
    "react-router": "^1.0.0-rc1",
    "react-search-bar": "^1.1.4",
    "react-tweet": "1.0.14",
    "redux": "3.1.5",
    "redux-router": "1.0.0-beta7",
    "redux-thunk": "^0.1.0",
    "redux-undo": "^0.5.0",
    "redux-wait": "0.0.3",
    "twit": "^2.1.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.0",
    "babel-runtime": "^5.8.20",
    "css-loader": "^0.9.0",
    "expect": "^1.12.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "jsdom-no-contextify": "^3.1.0",
    "merge": "^1.2.0",
    "mitm": "^1.2.0",
    "mocha": "^2.1.0",
    "nock": "^3.4.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.1.0",
    "redux-devtools": "^3.0.2",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^2.0.2",
    "redux-mock-store": "0.0.3",
    "style-loader": "^0.8.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
